    "build:prod": "ng build --project smartdeep --prod --aot && ng run smartdeep:server:production",
    "build:prod:watch": "ng build --project smartdeep --prod --watch && ng run smartdeep:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
